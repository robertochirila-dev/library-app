{
    "version": 2,
    "builds": [
      {
        "src": "backend/**/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "backend/app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/.*",
        "dest": "/backend/app.py"
      },
      {
        "src": "/(.*)",
        "dest": "/backend/$1"
      }
    ]
  }